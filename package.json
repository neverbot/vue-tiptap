{
  "name": "vue-tiptap",
  "version": "3.0.0",
  "description": "Vue component to use TipTap Editor without being renderless",
  "keywords": [
    "vue",
    "tiptap",
    "vuejs"
  ],
  "bugs": {
    "url": "https://github.com/neverbot/vue-tiptap/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neverbot/vue-tiptap"
  },
  "license": "MIT",
  "author": "neverbot <ivan@neverbot.com> (http://neverbot.com)",
  "exports": {
    ".": {
      "import": "./dist/vue-tiptap.js",
      "require": "./dist/vue-tiptap.umd.cjs"
    }
  },
  "main": "./dist/vue-tiptap.umd.cjs",
  "module": "./dist/vue-tiptap.js",
  "scripts": {
    "build": "npx vite build",
    "dev": "npx vite",
    "lint": "npx eslint . --ext js,json,vue",
    "lint:fix": "npx eslint . --ext js,json,vue --fix",
    "serve": "npx vite preview",
    "test": "echo \"Error: no tests\" && exit 1"
  },
  "dependencies": {
    "@tiptap/extension-underline": "^2.1.16",
    "@tiptap/starter-kit": "^2.1.16",
    "@tiptap/vue-3": "^2.1.16",
    "core-js": "^3.35.1",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/babel-preset-app": "^5.0.8",
    "@vue/compiler-sfc": "^3.4.15",
    "babel-loader": "^9.1.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.2.4",
    "vite": "^5.0.12",
    "vue-eslint-parser": "^9.4.2"
  },
  "engines": {
    "node": "^14 || ^16 || ^18 || ^20",
    "npm": "^7 || ^8 || ^9 || ^10"
  }
}
