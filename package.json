{
  "name": "vue-tiptap",
  "version": "3.1.2",
  "description": "Vue component to use TipTap Editor without being renderless",
  "keywords": [
    "vue",
    "tiptap",
    "vuejs"
  ],
  "bugs": {
    "url": "https://github.com/neverbot/vue-tiptap/issues"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/neverbot/vue-tiptap"
  },
  "license": "MIT",
  "author": "neverbot <ivan@neverbot.com> (http://neverbot.com)",
  "exports": {
    ".": {
      "import": "./dist/vue-tiptap.js",
      "require": "./dist/vue-tiptap.umd.cjs"
    }
  },
  "main": "./dist/vue-tiptap.umd.cjs",
  "module": "./dist/vue-tiptap.js",
  "scripts": {
    "build": "npx vite build",
    "dev": "npx vite",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix .",
    "serve": "npx vite preview",
    "test": "echo \"Error: no tests\" && exit 1"
  },
  "dependencies": {
    "@tiptap/extension-underline": "^2.8.0",
    "@tiptap/starter-kit": "^2.8.0",
    "@tiptap/vue-3": "^2.8.0",
    "vue": "^3.5.10"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/preset-env": "^7.25.4",
    "@eslint/js": "^9.11.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "babel-loader": "^9.2.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.10.0",
    "jsonc-eslint-parser": "^2.4.0",
    "prettier": "^3.3.3",
    "sass": "^1.79.4",
    "vite": "^5.4.8",
    "vue-eslint-parser": "^9.4.3"
  },
  "engines": {
    "node": "^14 || ^16 || ^18 || ^20",
    "npm": "^7 || ^8 || ^9 || ^10"
  }
}
